  "scripts": {
    "start": "next start",
    "build": "next build",
    "dev": "next dev",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm run dev",
    "build:tailwind": "tailwind build styles/index.css -o styles/tailwind.css",
    "format": "prettier --loglevel=warn --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js --fix",
    "codegen": "graphql-codegen --config codegen.yml && prettier --write ./graphql/ ",
    "test:lint": "yarn lint && yarn tsc",
    "test:jest": "yarn jest",
    "test": "yarn test:lint && yarn test:jest",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
